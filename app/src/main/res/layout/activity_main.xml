<?xml version="1.0" encoding="utf-8"?><android.support.constraint.ConstraintLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    tools:context="msmk.kindergarten.com.greendaotest.MainActivity">    <android.support.v7.widget.AppCompatButton        android:id="@+id/btn_insert"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_constraintTop_toTopOf="parent"        android:text="插入user信息"        />    <android.support.v7.widget.AppCompatButton        android:id="@+id/btn_quire"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_constraintTop_toBottomOf="@+id/btn_insert"        android:text="查询user信息"        />    <TextView        android:id="@+id/tv_result"        android:layout_width="match_parent"        android:layout_height="wrap_content"        app:layout_constraintTop_toBottomOf="@+id/btn_quire"        />    <VideoView        android:id="@+id/video"        android:layout_width="match_parent"        android:layout_height="200dp"        app:layout_constraintTop_toBottomOf="@+id/tv_result"/></android.support.constraint.ConstraintLayout>